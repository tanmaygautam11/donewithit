{
  "expo": {
    "name": "DoneWithIt",
    "slug": "DoneWithIt",
    "version": "1.0.0",
    "scheme": "donewithit",
    "orientation": "portrait",
    "icon": "./app/assets/logo.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo.png",
        "backgroundColor": "#FFEFEF"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
